{
	"comments": {
		"lineComment": "#"
	},
	"autoCloseBefore": ";:.,=}])> \n\t",
	"brackets": [
		["[", "]"]
	],
	"autoClosingPairs": [
		{ "open": "\"", "close": "\"", "notIn": ["string"] },
		{ "open": "'", "close": "'", "notIn": ["string", "comment"] },
		{ "open": "[", "close": "]" },
		{ "open": "{", "close": "}" }
	],
	"surroundingPairs": [
		["\"", "\""],
		["'", "'"],
		["(", ")"],
		["[", "]"],
		["{", "}"]
	]
}
