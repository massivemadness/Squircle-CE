{
  "name": "IntelliJ Light",
  "author": "Squircle CE",
  "type": "light",
  "colors": {
    "global.colorPrimary": "#3E73B9",
    "global.colorOutline": "#E0E0E0",
    "global.colorBackgroundPrimary": "#FFFFFF",
    "global.colorBackgroundSecondary": "#FFFFFF",
    "global.colorBackgroundTertiary": "#F0F1F4",
    "global.colorTextAndIconPrimary": "#000000",
    "global.colorTextAndIconPrimaryInverse": "#FFFFFF",
    "global.colorTextAndIconSecondary": "#7F8290",
    "global.colorTextAndIconDisabled": "#A9ADBC",
    "global.colorTextAndIconAdditional": "#8CCEF7",
    "global.colorTextAndIconSuccess": "#008860",
    "global.colorTextAndIconError": "#CD0000",
    "editor.background": "#FFFFFF",
    "editor.foreground": "#000000",
    "editor.whitespace": "#ADADAD",
    "editor.cursor": "#000000",
    "editor.handle": "#3E73B9",
    "editor.selection.background": "#AFD1FB",
    "editor.currentLine.background": "#FCFAEE",
    "editor.highlightedDelimiters.background": "#B5D5FB",
    "editor.highlightedDelimiters.foreground": "#000000",
    "editor.matchHighlight.background": "#E2FEDE",
    "editor.lineNumber.divider": "#E0E0E0",
    "editor.lineNumber.background": "#FFFFFF",
    "editor.lineNumber.foreground": "#ADADAD",
    "editor.lineNumber.activeForeground": "#828282",
    "editor.indentGuide.background": "#E0E0E0",
    "editor.indentGuide.activeBackground": "#828282",
    "editor.popupWindow.corner": "#E0E0E0",
    "editor.popupWindow.background": "#FFFFFF",
    "editor.popupWindow.activeBackground": "#F0F1F4"
  },
  "tokenColors": [
    {
      "settings": {
        "background": "#FFFFFF",
        "foreground": "#000000"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#8C8C8C"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#377B2A"
      }
    },
    {
      "name": "String Interpolation",
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#1232AC"
      }
    },
    {
      "name": "String Interpolation Body",
      "scope": "expression.string,meta.template.expression",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#284FE2"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language,variable.language,constant.character.escape",
      "settings": {
        "foreground": "#1232AC"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character,constant.other",
      "settings": {
        "foreground": "#284FE2"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword,keyword.operator.new,keyword.operator.delete,keyword.operator.expression",
      "settings": {
        "foreground": "#1232AC"
      }
    },
    {
      "name": "Types, Class Types",
      "scope": "storage.type.generic,meta.return.type,meta.type.annotation,meta.type.parameters,support.type.primitive",
      "settings": {
        "foreground": "#284FE2"
      }
    },
    {
      "name": "Storage Type",
      "scope": "storage,storage.type,storage.modifier",
      "settings": {
        "foreground": "#1232AC"
      }
    },
    {
      "name": "Class constructor",
      "scope": "new.expr entity.name.type",
      "settings": {
        "foreground": "#1232AC"
      }
    },
    {
      "name": "Function",
      "scope": "support.function,entity.name.function",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Function Argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Variable, Property",
      "scope": "variable.property,variable.other.property,variable.other.object.property,variable.object.property,support.variable.property,meta.object-literal.key",
      "settings": {
        "foreground": "#7C1E8F"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#1232AC"
      }
    },
    {
      "name": "Markup Links",
      "scope": "markup.underline.link",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#377B2A"
      }
    },
    {
      "name": "Tag name",
      "scope": "punctuation.definition.tag,entity.name.tag",
      "settings": {
        "foreground": "#1232AC"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#2649CC"
      }
    },
    {
      "name": "Tag value",
      "scope": "support.constant.property-value,string.quoted.single.tag,string.quoted.double.tag,string.unquoted.tag",
      "settings": {
        "foreground": "#377B2A"
      }
    },
    {
      "name": "Documentation Block",
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#8C8C8C"
      }
    },
    {
      "name": "Documentation Highlight",
      "scope": "keyword.other.documentation",
      "settings": {
        "foreground": "#8C8C8C"
      }
    },
    {
      "name": "Documentation Entity",
      "scope": "entity.name.type.instance.jsdoc",
      "settings": {
        "foreground": "#1232AC"
      }
    },
    {
      "name": "Regex",
      "scope": "string.regexp,punctuation.definition.group.regexp,punctuation.definition.character-class.regexp",
      "settings": {
        "foreground": "#1232AC"
      }
    },
    {
      "name": "Annotation, Preprocessor",
      "scope": "meta.preprocessor,entity.name.other.preprocessor,storage.type.annotation,punctuation.definition.annotation",
      "settings": {
        "foreground": "#9A892E"
      }
    },
    {
      "name": "CSS ID, Selector",
      "scope": "meta.selector.css,entity.other.attribute-name.class,entity.other.attribute-name.pseudo-class.css,entity.other.attribute-name.pseudo-element.css",
      "settings": {
        "foreground": "#1232AC"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    },
    {
      "name": "Ignored (Exceptions Rules)",
      "scope": "punctuation.definition.typeparameters.begin,punctuation.definition.typeparameters.end",
      "settings": {
        "foreground": "#000000"
      }
    }
  ],
  "semanticHighlighting": true
}
